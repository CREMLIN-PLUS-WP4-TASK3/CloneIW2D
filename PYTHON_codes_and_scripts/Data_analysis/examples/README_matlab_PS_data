# example
import scipy.io as sio

cd /media/b47b52a4-e1bb-4c0a-83cf-48aca3c6ec91/Mesures_PS/instabilities_damper_MDs_Jan_Fev_2013/MD15Feb2013

file='2013.02.15.08.36.15.322.mat'

mat=sio.loadmat(file,struct_as_record=False, squeeze_me=True)

data=mat['data']

plot(data.PR.SCOPE31.CH01.Acquisition.value)

data.PA.TFB_DSPU_H.SwitchesAcq.openLoop

plot(data.PR.BQSB.Acquisition.rawDataH)


# to superpose traces (here 50 traces)
plot(reshape(data.PR.SCOPE31.CH01.Acquisition.value,(50,200)).transpose())
